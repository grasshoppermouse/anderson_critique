@ARTICLE{Van_Der_Sluys1996-yt,
  title    = "Use and Trade of Rattan by the {Jahai} in {Hulu Perak, Malaysia}",
  author   = "Van Der Sluys, Corry",
  abstract = "This article is about rattan collecting activities as practised
